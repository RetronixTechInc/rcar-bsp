+                            308   315
+                            596   618
+                            898   934
+                            965   1044
+                            1024  1286
+                    >;
+                    idle-cost-data = <
+                            71
+                            71
+                            0
+                            0
+                    >;
+            };
+            CPU_COST_1: core-cost1 {
+                    busy-cost-data = <
+                            379   131
+                    >;
+                    idle-cost-data = <
+                            17
+                            17
+                            0
+                            0
+                    >;
+            };
+            CLUSTER_COST_0: cluster-cost0 {
+                    busy-cost-data = <
+                            308   33
+                            596   71
+                            898   112
+                            965   126
+                            1024  154
+                    >;
+                    idle-cost-data = <
+                            112
+                            112
+                            0
+                            0
+                    >;
+            };
+            CLUSTER_COST_1: cluster-cost1 {
+                    busy-cost-data = <
+                            379   16
+                    >;
+                    idle-cost-data = <
+                            17
+                            17
+                            0
+                            0
+                    >;
+            };
+	};
+
+	mmngr {
+		compatible = "renesas,mmngr";
+		memory-region = <&mmp_reserved>, <&lossy_decompress>;
+	};
+
+	mmngrbuf {
+		compatible = "renesas,mmngrbuf";
+	};
+
+	vspm_if {
+		compatible = "renesas,vspm_if";
+	};
+	
