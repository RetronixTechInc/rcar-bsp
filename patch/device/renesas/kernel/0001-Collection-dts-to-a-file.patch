+/ {
+	model = "Retronix IVI board based on r8a7795 ES3.0+ with 16GiB (4x4 GiB)";
+	compatible = "renesas,salvator-xs", "renesas,r8a7795";
+
+	aliases {
+		serial0 = &scif2;
+		serial1 = &scif1;
+		ethernet0 = &avb;
+	};
+
+	chosen {
+		bootargs = "";
+		stdout-path = "serial0:115200n8";
+	};
+
+	memory@48000000 {
+		device_type = "memory";
+		/* first 128MB is reserved for secure area. */
+		reg = <0x0 0x48000000 0x0 0x78000000>;
+	};
+
+	memory@400000000 {
+		device_type = "memory";
+		reg = <0x4 0x80000000 0x0 0x80000000>;
+	};
+
+	memory@500000000 {
+		device_type = "memory";
+		reg = <0x5 0x00000000 0x1 0x00000000>;
+	};
+
+	memory@600000000 {
+		device_type = "memory";
+		reg = <0x6 0x00000000 0x1 0x00000000>;
+	};
+
+	memory@700000000 {
+		device_type = "memory";
+		reg = <0x7 0x00000000 0x1 0x00000000>;
+	};
+
+	memory_ch1: memory@48000000 {
+		device_type = "memory";
+		/* first 128MB is reserved for secure area. */
+		reg = <0x0 0x48000000 0x0 0x78000000>;
+	};
+
+	memory_ch2: memory@400000000 {
+		device_type = "memory";
+		reg = <0x4 0x80000000 0x0 0x80000000>;
+	};
+
+	memory_ch3: memory@500000000 {
+		device_type = "memory";
+		reg = <0x5 0x00000000 0x1 0x00000000>;
+	};
+
+	memory_ch4: memory@600000000 {
+		device_type = "memory";
+		reg = <0x6 0x00000000 0x1 0x00000000>;
+	};
+
+	memory_ch5: memory@700000000 {
+		device_type = "memory";
+		reg = <0x7 0x00000000 0x1 0x00000000>;
+	};
+
+	ion {
+		compatible = "renesas,ion-rcar";
+
+		ion_heap {
+			compatible = "renesas,ion-rcar-heap";
+			memory-region = <&ion_reserved>;
+			heap-name = "rcar-ion";
+		};
+	};
+
+	reserved-memory {
+		#address-cells = <2>;
+		#size-cells = <2>;
+		ranges;
+
+		ramoops@4fe00000 {
+			compatible = "ramoops";
+			reg = <0x00000000 0x4FE00000 0x0 0x0100000>; /* 1MB */
+			record-size = <0x40000>;
+			console-size = <0x80000>;
+		};
+
+		rambootreason {
+			reg = <0x00000000 0x4FF00000 0x0 0x00001000>;
+		};
+
+		/* global autoconfigured region for contiguous allocations */
+		cma_reserved: linux,cma@50000000 {
+			compatible = "shared-dma-pool";
+			reusable;
+			reg = <0x00000000 0x50000000 0x0 0x04000000>; /* 64Mb */
+			linux,cma-default;
+		};
+
+		/* device specific region for Lossy Decompression */
+		lossy_decompress: linux,lossy_decompress@54000000 {
+			no-map;
+			reg = <0x00000000 0x54000000 0x0 0x03000000>;
+		};
+
+		adsp_reserved: linux,adsp@57000000 {
+			compatible = "shared-dma-pool";
+			reusable;
+			reg = <0x00000000 0x57000000 0x0 0x01000000>;
+		};
+
+
+		/* device specific region for contiguous allocations */
+		mmp_reserved: linux,multimedia@58000000 {
+			compatible = "shared-dma-pool";
+			reusable;
+			reg = <0x00000000 0x58000000 0x0 0x10000000>; /* 256Mb */
+		};
+
+		ion_reserved: linux,ion@68000000 {
+		    compatible = "ion-region";
+			reg = <0x00000000 0x68000000 0x0 0x20000000>; /* 512Mb */
+		};
+	};
+
+	cpus {
+		cpu-map {
+			cluster0 {
+				core0 {
+					cpu = <&a57_0>;
+				};
+				core1 {
+					cpu = <&a57_1>;
+				};
+				core2 {
+					cpu = <&a57_2>;
+				};
+				core3 {
+					cpu = <&a57_3>;
+				};
+			};
+
+			cluster1 {
+				core0 {
+					cpu = <&a53_0>;
+				};
+				core1 {
+					cpu = <&a53_1>;
+				};
+				core2 {
+					cpu = <&a53_2>;
+				};
+				core3 {
+					cpu = <&a53_3>;
+				};
+			};
+		};
+	};
+
